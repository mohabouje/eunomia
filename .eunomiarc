gitignore:
    languages:
        - python
        - linux
        - macos
    workspace:
        - .vscode
        - .venv
        - .build
gitlint:
    title:
        regex: '^EIN-\d+(\s\[(wip|dev|test|doc|fix|feat|style|refact|error)\])?\s[A-Z].*$'
        ignore: '^Release(.*)'
        min_length: 10
        max_length: 80
    body:
        min_length: 10
        max_length: 512
    author:
        email: '[^@]+@gmail.com'
